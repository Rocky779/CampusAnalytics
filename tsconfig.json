{
	"name": "330Project",
  	"version": "1.0.0",
	"extends": "@tsconfig/node18/tsconfig.json",
	"compilerOptions": {
		"outDir": "dist",
		"noImplicitAny": true,
		"removeComments": true,
		"preserveConstEnums": true,
		"sourceMap": true,
		"typeRoots": [
			"./node_modules/@types"
		]
	}, "dependencies": {
		"@tsconfig/node18": "^18.2.2",
		"@types/chai": "^4.3.11",
		"@types/chai-as-promised": "^7.1.8",
		"@types/fs-extra": "^11.0.4",
		"@types/mocha": "^10.0.6",
		"@types/node": "^20.11.0",
		"chai": "4.3.10",
		"chai-as-promised": "^7.1.1",
		"fs-extra": "^11.2.0",
		"jszip": "^3.10.1",
		"mocha": "^10.2.0",
		"nyc": "^15.1.0",
		"ts-node": "^10.9.2",
		"typescript": "^5.3.3"
	  },
	"include": [
		"src/**/*.ts",
		"test/*.ts",
		"test/**/*.ts",
	],
	"exclude": [
		"node_modules"
